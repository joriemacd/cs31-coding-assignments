    "def count_cross_inv(P, Q):\n",
    "    # P and Q are sorted arrays; outputs the number of (i,j) with P[i]>Q[j]\n",
    "    i = 0; j = 0; num = 0\n",
    "    p = len(P); q = len(Q)\n",
    "    while (i < p) and (j < q):\n",
    "        if (P[i] > Q[j]):\n",
    "            num = num + p - i\n",
    "            j += 1\n",
    "            #break\n",
    "        else:\n",
    "            i += 1\n",
    "    return num;"
